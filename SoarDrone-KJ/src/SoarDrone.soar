sp {cleanOutput*down
    (state <s> ^superstate nil
               ^operator <o>
               ^io.output-link <out>)
    (<out> ^down false)
    (<o> ^name move)
-->
    (<out> ^direction down -)
}

sp {cleanOutput*nothing
    (state <s> ^superstate nil
               ^operator <o>
               ^io.output-link <out>)
    (<out> ^down false)
    (<o> ^name move)
-->
    (<out> ^direction nothing -)
}

