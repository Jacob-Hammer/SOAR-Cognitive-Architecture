sp {propose*moveDown
    (state <s> ^superstate nil)
    (<s> ^io.input-link <input>)
    (<input> ^position <pos>)
    (<pos> ^yValue <yVal> > 540)
#    (<pos> ^speed <speed>)
-->
    (<s> ^operator <o> + =)
    (<o> ^name moveDown)
    (<o> ^yValue <yVal>)
#    (<o> ^speed <speed>)
}

sp {apply*moveDown
    (state <s> ^operator <o>)
    (<o> ^name moveDown)
    (<s> ^io.output-link <output>)
    (<o> ^yValue <yVal>)
#    (<o> ^speed <speed>)
-->
    (<output> ^direction down)
}

