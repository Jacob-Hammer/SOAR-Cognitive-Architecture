sp {cleanOutput*up
    (state <s> ^superstate nil
               ^operator <o>
               ^io.output-link <out>)
    (<out> ^up false)
    (<o> ^name move)
-->
    (<out> ^direction up -)
}

sp {cleanOutput*left
    (state <s> ^superstate nil
               ^operator <o>
               ^io.output-link <out>)
    (<out> ^left true)
    (<o> ^name doNothing)
-->
    (<out> ^direction left -)
}

sp {cleanOutput*right
    (state <s> ^superstate nil
               ^operator <o>
               ^io.output-link <out>)
    (<out> ^right true)
