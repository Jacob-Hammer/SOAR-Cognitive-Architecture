sp {propose*breakImpasse-down-right
    (state <s> ^impasse tie)
    (<s> ^item <o1>)
    (<s> ^item <o2>)
    (<o1> ^name moveDown)
    (<o2> ^name moveRight)
    
--> 
    (<s> ^operator <o> + =)
    (<o> ^name breakImpasse-down-right)
    (<o> ^name1 moveDown)
    (<o> ^name2 moveRight)    
} 

sp {apply*breakImpasse-down-right
    (state <s> ^operator <o>)
    (<o> ^name breakImpasse)
    (<o> ^name1 moveDown)
    (<o> ^name2 moveRight)

    (<s> ^superstate <ss>)
    (<ss> ^operator <o1> +)
    (<o1> ^name moveDown)
    (<ss> ^operator <o2> +)
    (<o2> ^name moveRight)
-->
    (<ss> ^operator <o2> !)
    (write |'breaking impasse'|)       
}

