sp {cleanOutput*up
    (state <s> ^superstate nil
               ^operator <o>
               ^io.output-link <out>)
    (<out> ^up false)
    (<o> ^name move)
-->
    (<out> ^up false -)
}

sp {cleanOutput*up
    (state <s> ^superstate nil
               ^operator <o>
               ^io.output-link <out>)
    (<out> ^up true)
    (<o> ^name doNothing)
-->
    (<out> ^up true -)
}

