sp {propose*doNothing
    (state <s> ^superstate nil)
    (<s> ^io.input-link <input>)
    (<input> ^position <pos>)
    (<pos> ^yValue <yVal> { > 540 < 300 })
    (<pos> ^xValue <xVal> { > 330 < 570 })
    (<pos> ^speed <speed>)
-->
    (<s> ^operator <o> + =)
    (<o> ^name move)
    (<o> ^yValue <yVal>)
    (<o> ^xValue <xVal>)
    (<o> ^speed <speed>)
}

sp {apply*doNothing
    (state <s> ^operator <o>)
    (<o> ^name move)
    (<s> ^io.output-link <output>)
    (<o> ^yValue <yVal>)
    (<o> ^xValue <xVal>)
    (<o> ^speed <speed>)
-->
    (<output> ^direction nothing)
}


